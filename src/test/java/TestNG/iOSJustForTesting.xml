        <parameter name="testDevice" value="iPhone XS Max" />
        <!--<parameter name="testDevice" value="REAL-ZiphoneX" />-->
        <!--<parameter name="testDevice" value="REAL-Ziphone8plus" />-->
        <!--<parameter name="testDevice" value="REAL-Z6plus" />-->
        <!--<parameter name="testDevice" value="REAL-Z6iphone" />-->
        <!--<parameter name="testDevice" value="REAL-Ziphone7" />-->
        <parameter name="startSleepTime" value="0200" />

        <groups>
            <run>
